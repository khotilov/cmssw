This test suite is meant to try the configurations which are used for the RelVal samples.

<var name="DBS_RELEASE" value="CMSSW_3_1_0_pre6">
<var name="DBS_LIKE" value="*IDEAL_31X_v1*-*RAW*">

<diffline expr="(Begin processing the \S* record)">
<diffline expr="(found \S* particles)">

<environment name="SingleElectronPt10Raw">
  <executable name="cmsDriver.py" args="SingleElectronPt10.cfi -s GEN:ProductionFilterSequence,SIM,DIGI,L1,DIGI2RAW -n 1000 --eventcontent FEVTDEBUG --conditions FrontierConditions_GlobalTag,IDEAL_31X::All --python_filename=SingleElectronPt10Raw.py --fileout=SingleElectronPt10Raw.root --no_exec">
  <executable name="cmsRun" args="SingleElectronPt10Raw.py">
</environment>

<environment name="SingleElectronPt10Reco">
  <executable name="cmsDriver.py" args="step2 -s RAW2DIGI,RECO --filein file:SingleElectronPt10Raw.root -n 1000 --datatier GEN-SIM-RECO --eventcontent RECOSIM --conditions FrontierConditions_GlobalTag,IDEAL_31X::All --python_filename=SingleElectronPt10Reco.py --fileout=SingleElectronPt10Reco.root --no_exec">
  <var name="DBS_SAMPLE" value="RelValSingleElectronPt10">
  <executable name="dbs_discovery.py">
  <executable name="cmsRun" args="SingleElectronPt10Reco.py">
</environment>

