This test suite is meant to try the configurations which are used for the RelVal samples.

# Temporary modifications to be added to reco python files
import sys
import os
sys.path.append('.')
import dbs_discovery
process.source = cms.Source ("PoolSource",
    fileNames = cms.untracked.vstring(),
	secondaryFileNames = cms.untracked.vstring()
)
process.source.fileNames.extend(dbs_discovery.search())
process.ecalDrivenElectronSeeds.SeedConfiguration.fromTrackerSeeds = cms.bool(False)


<var name="DBS_RELEASE" value="CMSSW_3_3_0_pre3">
<var name="DBS_COND" value="MC_31X_*">
<var name="DBS_TIER" value="-*RAW*">

<diffline expr="(Begin processing the \S* record)">
<diffline expr="(found \S* particles)">

<environment name="SingleElectronPt10Raw">
  <executable name="cmsDriver.py" args="SingleElectronPt10.cfi -s GEN:ProductionFilterSequence,SIM,DIGI,L1,DIGI2RAW -n 1000 --eventcontent FEVTDEBUG --conditions FrontierConditions_GlobalTag,MC_31X_V1::All --python_filename=SingleElectronPt10Raw.py --fileout=RelValSingleElectronPt10-RAW.root --mc --no_exec">
  <executable name="cmsRun" args="SingleElectronPt10Raw.py">
</environment>

<environment name="SingleElectronPt10Reco">
  <executable name="cmsDriver.py" args="step2 -s RAW2DIGI,RECO --filein file:RelValSingleElectronPt10-RAW.root -n 1000 --datatier GEN-SIM-RECO --eventcontent RECOSIM --conditions FrontierConditions_GlobalTag,MC_31X_V1::All --python_filename=SingleElectronPt10Reco.py --fileout=RelValSingleElectronPt10-RECO.root --no_exec">
  <var name="DBS_SAMPLE" value="RelValSingleElectronPt10">
  <executable name="dbs_discovery.py">
  <executable name="cmsRun" args="SingleElectronPt10Reco.py">
</environment>

