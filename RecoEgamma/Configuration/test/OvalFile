This test suite is meant to check one has not broken the
configurations used for the RelVal samples. It is trying
SingleElectronPt35 and QcdPt80-120 with 10 events.
Type "oval p".

<var name="MY_GLOBAL_TAG" value="MC_31X_V9">

<diffline expr="(Begin processing the \S* record)">

<environment name="SingleElectronPt35">

  <environment name="SingleElectronPt35Raw">
    <executable name="cmsDriver.py" args="SingleElectronPt35.cfi -s GEN,SIM,DIGI,L1,DIGI2RAW,HLT -n 10 --eventcontent FEVTDEBUGHLT --conditions FrontierConditions_GlobalTag,${MY_GLOBAL_TAG}::All --python_filename=SingleElectronPt35Raw.py --fileout=SingleElectronPt35Raw.root">
  </environment>
  
  <environment name="SingleElectronPt35Reco">
    <executable name="cmsDriver.py" args="step2 -s RAW2DIGI,RECO,VALIDATION --filein file:SingleElectronPt35Raw.root -n 10 --datatier GEN-SIM-RECO --eventcontent RECOSIM --conditions FrontierConditions_GlobalTag,${MY_GLOBAL_TAG}::All --python_filename=SingleElectronPt35Reco.py --fileout=SingleElectronPt35Reco.root">
  </environment>
  
  <environment name="SingleElectronPt35Harv">
    <executable name="cmsDriver.py" args="step3 -s HARVESTING:validationHarvesting --filein file:SingleElectronPt35Reco.root  --harvesting AtJobEnd --conditions FrontierConditions_GlobalTag,${MY_GLOBAL_TAG}::All --python_filename=SingleElectronPt35Harv.py --no_exec">
    <executable name="cmsRun" args="MySingleElectronPt35Harv.py">
  </environment>

</environment>

<environment name="QcdPt80-120">

  <environment name="QcdPt80-120Raw">
    <executable name="cmsDriver.py" args="QCD_Pt_80_120.cfi -s GEN,SIM,DIGI,L1,DIGI2RAW,HLT -n 10 --eventcontent FEVTDEBUGHLT --conditions FrontierConditions_GlobalTag,${MY_GLOBAL_TAG}::All --python_filename=QcdPt80-120Raw.py --fileout=QcdPt80-120Raw.root">
  </environment>
  
  <environment name="QcdPt80-120Reco">
    <executable name="cmsDriver.py" args="step2 -s RAW2DIGI,RECO,VALIDATION --filein file:QcdPt80-120Raw.root -n 10 --datatier GEN-SIM-RECO --eventcontent RECOSIM --conditions FrontierConditions_GlobalTag,${MY_GLOBAL_TAG}::All --python_filename=QcdPt80-120Reco.py --fileout=QcdPt80-120Reco.root">
  </environment>
  
  <environment name="QcdPt80-120Harv">
    <executable name="cmsDriver.py" args="step3 -s HARVESTING:validationHarvesting --filein file:QcdPt80-120Reco.root  --harvesting AtJobEnd --conditions FrontierConditions_GlobalTag,${MY_GLOBAL_TAG}::All --python_filename=QcdPt80-120Harv.py --no_exec">
    <executable name="cmsRun" args="MyQcdPt80-120Harv.py">
  </environment>

</environment>
  
