<TESTSCONFIGURATION> 
<QTEST name="MeanWithinExpectedRange:Digi" activate="true">
      <TYPE>MeanWithinExpected</TYPE>
      <PARAM name="error">0.05</PARAM>
      <PARAM name="warning">0.3</PARAM>
      <PARAM name="mean">1.0</PARAM>
      <PARAM name="useRMS">0</PARAM>
      <PARAM name="useSigma">0</PARAM>
      <PARAM name="useRange">1</PARAM>
      <PARAM name="xmin">0.0</PARAM>
      <PARAM name="xmax">0.4</PARAM>
</QTEST>
<QTEST name="ContentsWithinExpected:TkHistoDigi">
        <TYPE>ContentsWithinExpected</TYPE>
        <PARAM name="minMean">0.0</PARAM>
        <PARAM name="maxMean">0.4</PARAM>
        <PARAM name="minRMS">0</PARAM>
        <PARAM name="maxRMS">0</PARAM>
        <PARAM name="toleranceMean">-1</PARAM>
        <PARAM name="useEmptyBins">1</PARAM>
        <PARAM name="minEntries">1000</PARAM>
        <PARAM name="error">0.30</PARAM>
        <PARAM name="warning">0.70</PARAM>
</QTEST>
<QTEST name="MeanWithinExpectedRange:Clusters" activate="true">
      <TYPE>MeanWithinExpected</TYPE>
      <PARAM name="error">0.05</PARAM>
      <PARAM name="warning">0.3</PARAM>
      <PARAM name="mean">1.0</PARAM>
      <PARAM name="useRMS">0</PARAM>
      <PARAM name="useSigma">0</PARAM>
      <PARAM name="useRange">1</PARAM>
      <PARAM name="xmin">0.0</PARAM>
      <PARAM name="xmax">0.1</PARAM>
</QTEST>
<QTEST name="ContentsWithinExpected:TkHistoCluster">
        <TYPE>ContentsWithinExpected</TYPE>
        <PARAM name="minMean">0.0</PARAM>
        <PARAM name="maxMean">0.1</PARAM>
        <PARAM name="minRMS">0</PARAM>
        <PARAM name="maxRMS">0</PARAM>
        <PARAM name="toleranceMean">-1</PARAM>
        <PARAM name="useEmptyBins">1</PARAM>
        <PARAM name="minEntries">30</PARAM>
        <PARAM name="error">0.30</PARAM>
        <PARAM name="warning">0.70</PARAM>
</QTEST>
<QTEST name="XrangeWithin:0-10.0" activate="true"> 
     <TYPE>ContentsXRange</TYPE> 
     <PARAM name="error">0.2</PARAM> 
     <PARAM name="warning">0.4</PARAM> 
     <PARAM name="xmin">0</PARAM> 
     <PARAM name="xmax">10.0</PARAM> 
</QTEST>
<QTEST name="XrangeWithin:TrkRecHit" activate="true">
     <TYPE>ContentsXRange</TYPE>
     <PARAM name="error">0.90</PARAM>
     <PARAM name="warning">0.95</PARAM>
     <PARAM name="xmin">5.0</PARAM>
     <PARAM name="xmax">45.0</PARAM>
</QTEST>
<QTEST name="XrangeWithin:Chi2/dof">
     <TYPE>ContentsXRange</TYPE>
     <PARAM name="error">0.90</PARAM>
     <PARAM name="warning">0.95</PARAM>
     <PARAM name="xmin">0.0</PARAM>
     <PARAM name="xmax">15.0</PARAM>
</QTEST>
<LINK name="*TkHMap_NumberOfDigi_*">
  <TestName activate="true">ContentsWithinExpected:TkHistoDigi</TestName>
</LINK>
<LINK name="*NumberOfDigis__det__*"> 
  <TestName activate="true">MeanWithinExpectedRange:Digi</TestName> 
</LINK>
<LINK name="*TkHMap_NumberOfCluster_*">
  <TestName activate="true">ContentsWithinExpected:TkHistoCluster</TestName>
</LINK>
<LINK name="*NumberOfClusters__det__*"> 
  <TestName activate="true">MeanWithinExpectedRange:Clusters</TestName> 
</LINK>
<LINK name="*NumberOfRecHitsPerTrack_*">
  <TestName activate="true">XrangeWithin:TrkRecHit</TestName>
</LINK>
<LINK name="*Chi2overDoF_*">
  <TestName activate="true">XrangeWithin:TrkRecHit</TestName>
</LINK>
</TESTCONFIGURATION>
