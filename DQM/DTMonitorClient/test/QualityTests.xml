<TESTSCONFIGURATION>

<QTEST name="tTrigOffSet">
	<TYPE>ContentsYRange</TYPE>	
	<PARAM name="ymin">-10</PARAM>
	<PARAM name="ymax">10</PARAM>
	<PARAM name="error">0.30</PARAM>
	<PARAM name="warning">0.70</PARAM>
</QTEST>


<QTEST name="ResidualsDistributionGaussianTest">
        <TYPE>MeanWithinExpected</TYPE>
        <PARAM name="error">0.05</PARAM>
        <PARAM name="warning">0.5</PARAM>
        <PARAM name="mean">0.0</PARAM>
        <PARAM name="useRMS">0</PARAM>
        <PARAM name="useSigma">0</PARAM>
        <PARAM name="useRange">1</PARAM>
        <PARAM name="xmin">-0.001</PARAM>
        <PARAM name="xmax">0.001</PARAM>
</QTEST>


<QTEST name="ResidualsMeanInRange">
	<TYPE>ContentsYRange</TYPE>	
	<PARAM name="ymin">-0.015</PARAM>
	<PARAM name="ymax">0.015</PARAM>
	<PARAM name="error">0.30</PARAM>
	<PARAM name="warning">0.70</PARAM>
</QTEST>


<QTEST name="ResidualsSigmaInRange">
	<TYPE>ContentsYRange</TYPE>	
	<PARAM name="ymin">0</PARAM>
	<PARAM name="ymax">0.07</PARAM>
	<PARAM name="error">0.30</PARAM>
	<PARAM name="warning">0.70</PARAM>
</QTEST>


<QTEST name="ResidualsSlopeInRange">
	<TYPE>ContentsYRange</TYPE>	
	<PARAM name="ymin">-0.05</PARAM>
	<PARAM name="ymax">0</PARAM>
	<PARAM name="error">0.30</PARAM>
	<PARAM name="warning">0.70</PARAM>
</QTEST>


<QTEST name="EfficiencyInRange">
	<TYPE>ContentsYRange</TYPE>	
	<PARAM name="ymin">0.98</PARAM>
	<PARAM name="ymax">1.00</PARAM>
	<PARAM name="error">0.30</PARAM>
	<PARAM name="warning">0.70</PARAM>
</QTEST>


<QTEST name="UnassEfficiencyInRange">
	<TYPE>ContentsYRange</TYPE>	
	<PARAM name="ymin">0.98</PARAM>
	<PARAM name="ymax">1.00</PARAM>
	<PARAM name="error">0.30</PARAM>
	<PARAM name="warning">0.70</PARAM>
</QTEST>


<QTEST name="OccupancyDiffInRange">
	<TYPE>DeadChannel</TYPE>	
	<PARAM name="threshold">0.05</PARAM>
	<PARAM name="error">0.30</PARAM>
	<PARAM name="warning">0.70</PARAM>
</QTEST>


<QTEST name="ChEfficiencyInRangeX">
	<TYPE>ContentsYRange</TYPE>	
	<PARAM name="ymin">0.98</PARAM>
	<PARAM name="ymax">1.00</PARAM>
	<PARAM name="error">0.03</PARAM>
	<PARAM name="warning">0.70</PARAM>
</QTEST>


<QTEST name="ChEfficiencyInRangeY">
	<TYPE>ContentsYRange</TYPE>	
	<PARAM name="ymin">0.98</PARAM>
	<PARAM name="ymax">1.00</PARAM>
	<PARAM name="error">0.30</PARAM>
	<PARAM name="warning">0.70</PARAM>
</QTEST>


<QTEST name="NoiseMeanInRange">
	<TYPE>ContentsYRange</TYPE>	
	<PARAM name="ymin">0.00</PARAM>
	<PARAM name="ymax">0.02</PARAM>
	<PARAM name="error">0.30</PARAM>
	<PARAM name="warning">0.70</PARAM>
</QTEST>


<QTEST name="SectorTrigEffPhiInRange">
	<TYPE>ContentsYRange</TYPE>	
	<PARAM name="ymin">0.5</PARAM>
	<PARAM name="ymax">1.00</PARAM>
	<PARAM name="error">0.95</PARAM>
	<PARAM name="warning">0.95</PARAM>
</QTEST>


<QTEST name="SectorCorrFractionInRange">
	<TYPE>ContentsYRange</TYPE>	
	<PARAM name="ymin">0.50</PARAM>
	<PARAM name="ymax">1.00</PARAM>
	<PARAM name="error">0.80</PARAM>
	<PARAM name="warning">0.90</PARAM>
</QTEST>


<QTEST name="SectorTrigPhiSlopeInRange">
	<TYPE>ContentsYRange</TYPE>	
	<PARAM name="ymin">0.80</PARAM>
	<PARAM name="ymax">1.20</PARAM>
	<PARAM name="error">0.80</PARAM>
	<PARAM name="warning">0.90</PARAM>
</QTEST>


<QTEST name="SectorTrigPhibSlopeInRange">
	<TYPE>ContentsYRange</TYPE>	
	<PARAM name="ymin">0.80</PARAM>
	<PARAM name="ymax">1.20</PARAM>
	<PARAM name="error">0.55</PARAM>
	<PARAM name="warning">0.70</PARAM>
</QTEST>


<QTEST name="SectorTrigPhiInterceptInRange">
	<TYPE>ContentsYRange</TYPE>	
	<PARAM name="ymin">-25</PARAM>
	<PARAM name="ymax">25</PARAM>
	<PARAM name="error">0.80</PARAM>
	<PARAM name="warning">0.90</PARAM>
</QTEST>


<QTEST name="SectorTrigPhibInterceptInRange">
	<TYPE>ContentsYRange</TYPE>	
	<PARAM name="ymin">-25</PARAM>
	<PARAM name="ymax">25</PARAM>
	<PARAM name="error">0.80</PARAM>
	<PARAM name="warning">0.90</PARAM>
</QTEST>


<QTEST name="SectorTrigEffThetaInRange">
	<TYPE>ContentsYRange</TYPE>	
	<PARAM name="ymin">0.5</PARAM>
	<PARAM name="ymax">1.00</PARAM>
	<PARAM name="error">0.95</PARAM>
	<PARAM name="warning">0.95</PARAM>
</QTEST>


<LINK name="*tTrigTest*">
	<TestName activate="true">tTrigOffSet</TestName>
</LINK>

<LINK name="*hResDist_*">
        <TestName activate="true">ResidualsDistributionGaussianTest</TestName>
</LINK>

<LINK name="*MeanTest*">
	<TestName activate="true">ResidualsMeanInRange</TestName>
</LINK>

<LINK name="*SigmaTest*">
	<TestName activate="true">ResidualsSigmaInRange</TestName>
</LINK>

<LINK name="*SlopeTest*">
	<TestName activate="true">ResidualsSlopeInRange</TestName>
</LINK>

<LINK name="*Efficiency_*">
	<TestName activate="true">EfficiencyInRange</TestName>
</LINK>

<LINK name="*UnassEfficiency_*">
	<TestName activate="true">UnassEfficiencyInRange</TestName>
</LINK>

<LINK name="*OccupancyDiff*">
	<TestName activate="true">OccupancyDiffInRange</TestName>
</LINK>

<LINK name="*xEfficiency_*">
	<TestName activate="true">ChEfficiencyInRangeX</TestName>
</LINK>

<LINK name="*yEfficiency_*">
	<TestName activate="true">ChEfficiencyInRangeY</TestName>
</LINK>

<LINK name="*MeanDigiPerEvent*">
	<TestName activate="true">NoiseMeanInRange</TestName>
</LINK>

<LINK name="*TrigEffSec_Phi_*">
	<TestName activate="true">SectorTrigEffPhiInRange</TestName>
</LINK>

<LINK name="*TrigEffSec_Theta_*">
	<TestName activate="true">SectorTrigEffThetaInRange</TestName>
</LINK>

<LINK name="*CorrFractionSec_Phi_*">
	<TestName activate="true">SectorCorrFractionInRange</TestName>
</LINK>

<LINK name="*PhiTkvsTrigSlope*">
	<TestName activate="true">SectorTrigPhiSlopeInRange</TestName>
</LINK>

<LINK name="*PhibTkvsTrigSlope*">
	<TestName activate="true">SectorTrigPhibSlopeInRange</TestName>
</LINK>

<LINK name="*PhiTkvsTrigIntercept*">
	<TestName activate="true">SectorTrigPhiInterceptInRange</TestName>
</LINK>

<LINK name="*PhibTkvsTrigIntercept*">
	<TestName activate="true">SectorTrigPhibInterceptInRange</TestName>
</LINK>


</TESTSCONFIGURATION>
