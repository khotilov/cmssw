 </PosPart>
 <PosPart copyNumber="1">
   <rParent name="OCMS"/>
   <rChild name="table"/>
  <rRotation name="R3"/>
  <Translation x="1*cm " y="2*cm " z="0*m  " />
 </PosPart>
</PosPartSection>

<RotationSection label="table2DWithMirror.xml">
 <Rotation name="R0" phiX="-45*deg" thetaX="90*deg" phiY="45*deg" thetaY="90*deg" phiZ="0*deg" thetaZ="0*deg" />
