<bin   name="analyzeTiming" file="analyzeTiming.cpp">
  <flags NO_TESTRUN="1"/>
  <use   name="DataFormats/HLTReco"/>
  <use   name="FWCore/FWLite"/>
  <use   name="FWCore/Framework"/>
  <use   name="rootcintex"/>
  <use   name="root"/>
</bin>

<architecture name="slc">                                                                                                                                                                                                        
<bin name="testTimer" file="timer.cc">
  <lib name="rt"/>
  <flags CXXFLAGS="-fopenmp"/>
  <flags NO_TESTRUN="1"/>
</bin>
</architecture>                                                                                                                                                                                                                  

<architecture name="osx">                                                                                                                                                                                                        
<bin name="testTimer" file="timer.cc">
  <flags CXXFLAGS="-fopenmp"/>
  <flags NO_TESTRUN="1"/>
</bin>
</architecture>                                                                                                                                                                                                                  
