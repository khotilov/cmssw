####################################################################################################################
#The 'simhit' type Stacked Tracker Stubs
# no algorithm need be included
include "L1TriggerOffline/TriggerSimulation/data/StackedTrackerLocalStub_SimBuilder.cfi"                     
include "L1TriggerOffline/TriggerSimulation/data/StackedTrackerGlobalStub_SimBuilder.cfi"                    
####################################################################################################################

####################################################################################################################
#The TrackTrigger Hits from pixel digis
#requires the SiPixelDigitizer to have been run
include "L1TriggerOffline/TriggerSimulation/data/TrackTriggerHitsFromPixelDigis.cfi"
####################################################################################################################

####################################################################################################################
#The 'TrackTriggerHit' type Stacked Tracker Stubs
#requires inclusion of a matching algorithm
#for example:
#	include "L1TriggerOffline/TriggerSimulation/data/TrackTriggerHitMatchingAlgorithm_a.cfi"                  
#	include "L1TriggerOffline/TriggerSimulation/data/TrackTriggerHitMatchingAlgorithm_globalgeometry.cfi"                     
#include "........"
include "L1TriggerOffline/TriggerSimulation/data/TrackTriggerLocalStubsFromPixelDigis.cfi"
#include "L1TriggerOffline/TriggerSimulation/data/TrackTriggerLocalStubsFromStackedTrackerLocalStubDigi.cfi"
include "L1TriggerOffline/TriggerSimulation/data/TrackTriggerGlobalStubs.cfi"                                
####################################################################################################################

sequence LocalStackedTrackerSimStubs = {
	StackedTrackerLocalStubSimBuilder
}

sequence AllStackedTrackerSimStubs = {
	LocalStackedTrackerSimStubs &
	StackedTrackerGlobalStubSimBuilder
}

sequence LocalStackedTrackerDigiStubs = {
	StackedTrackerLocalStubDigiBuilder
}

sequence AllStackedTrackerDigiStubs = {
	LocalStackedTrackerDigiStubs &
	StackedTrackerGlobalStubDigiBuilder
}

sequence TrackTriggerHits = {
	TrackTriggerHitsFromPixelDigis
}

sequence LocalTrackTriggerHitStubs = {
	TrackTriggerHits &
	TrackTriggerLocalStubsFromPixelDigis
}

#sequence LocalTrackTriggerHitStubsFromStackedTrackerLocalDigiStubs = {
#	StackedTrackerLocalDigiStubs &
#	TrackTriggerLocalStubsFromStackedTrackerLocalStubDigi
#}

sequence AllTrackTriggerHitStubs = {
	LocalTrackTriggerHitStubs &
	TrackTriggerGlobalStubs
}

sequence AllLocalStubs = {
	LocalStackedTrackerSimStubs &
	LocalStackedTrackerDigiStubs &
	LocalTrackTriggerHitStubs
}

sequence AllStubs = {
	AllStackedTrackerSimStubs &
	AllStackedTrackerDigiStubs &
	AllTrackTriggerHitStubs
}


