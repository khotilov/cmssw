#requires the SiPixelDigitizer to have been run
include "L1TriggerOffline/TriggerSimulation/data/StackedTrackerLocalStub_DigiBuilder.cfi"                    
include "L1TriggerOffline/TriggerSimulation/data/StackedTrackerGlobalStub_DigiBuilder.cfi"                   
