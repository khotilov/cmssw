#
# required PU level must be set in FullSim_SingleMuon.py
#

# 140 GeV
sed s/PTMU/"140"/g FullSim_SingleMuon.py > 140GeV_FullSim_SingleMuon.py
cmsRun 140GeV_FullSim_SingleMuon.py 
mv 140GeV_FullSim_SingleMuon.py /lustre/cmsdata/zotto/SLHCdata/
# rm  /lustre/cmsdata/zotto/SLHCdata/out_mu_*_PU0.root

# 120 GeV
sed s/PTMU/"120"/g FullSim_SingleMuon.py > 120GeV_FullSim_SingleMuon.py
cmsRun 120GeV_FullSim_SingleMuon.py 
mv 120GeV_FullSim_SingleMuon.py /lustre/cmsdata/zotto/SLHCdata/
# rm  /lustre/cmsdata/zotto/SLHCdata/out_mu_*_PU0.root

# 100 GeV
sed s/PTMU/"100"/g FullSim_SingleMuon.py > 100GeV_FullSim_SingleMuon.py
cmsRun 100GeV_FullSim_SingleMuon.py 
mv 100GeV_FullSim_SingleMuon.py /lustre/cmsdata/zotto/SLHCdata/
# rm  /lustre/cmsdata/zotto/SLHCdata/out_mu_*_PU0.root

# 80 GeV
sed s/PTMU/"80"/g FullSim_SingleMuon.py > 80GeV_FullSim_SingleMuon.py
cmsRun 80GeV_FullSim_SingleMuon.py 
mv 80GeV_FullSim_SingleMuon.py /lustre/cmsdata/zotto/SLHCdata/
# rm  /lustre/cmsdata/zotto/SLHCdata/out_mu_*_PU0.root

# 70 GeV
sed s/PTMU/"70"/g FullSim_SingleMuon.py > 70GeV_FullSim_SingleMuon.py
cmsRun 70GeV_FullSim_SingleMuon.py 
mv 70GeV_FullSim_SingleMuon.py /lustre/cmsdata/zotto/SLHCdata/
# rm  /lustre/cmsdata/zotto/SLHCdata/out_mu_*_PU0.root

