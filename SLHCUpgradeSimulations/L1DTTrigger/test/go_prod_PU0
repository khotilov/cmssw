
# 140 GeV
sed s/PTMU/"140"/g DTSeededTrackletsProdGO_cfg.py > 140GeV_DTSeededTrackletsProdGO_cfg.py
sed s/PU/"PU0"/g 140GeV_DTSeededTrackletsProdGO_cfg.py > 140GeV_DTSeededTrackletsProdGO_PU0_cfg.py
cmsRun 140GeV_DTSeededTrackletsProdGO_PU0_cfg.py 
rm 140GeV_DTSeededTrackletsProdGO_cfg.py
mv 140GeV_DTSeededTrackletsProdGO_PU0_cfg.py /lustre/cmswork/zotto/SLHCdata/
rm  /lustre/cmswork/zotto/SLHCdata/out_mu_*_PU0.root

