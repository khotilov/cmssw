<use name="FWCore/MessageLogger"/>
<use name="FWCore/ParameterSet"/>
<use name="FWCore/ServiceRegistry"/>
<use name="FWCore/Utilities"/>
<use name="GeneratorInterface/LHEInterface"/>
<use name="boost"/>
<use name="hepmc"/>
<library name="GeneratorInterfaceLHEProducer" file="LHEProducer.cc LHEFilter.cc">
	<use name="FWCore/Framework"/>
	<use name="SimDataFormats/HepMCProduct"/>
	<flags EDM_PLUGIN="1"/>
	<flags CXXFLAGS="-O0 -fno-inline -g3"/>
</library>
<library name="GeneratorInterfaceLHESource" file="LHESource.cc MCDBSource.cc">
	<use name="FWCore/Framework"/>
	<use name="mcdb"/>
	<flags EDM_PLUGIN="1"/>
</library>
<library name="GeneratorInterfaceLHEAnalyzer" file="LHEAnalyzer.cc">
	<use name="FWCore/Framework"/>
	<use name="DataFormats/StdDictionaries"/>
	<use name="SimDataFormats/HepMCProduct"/>
	<use name="PhysicsTools/UtilAlgos"/>
	<flags EDM_PLUGIN="1"/>
</library>
<library name="GeneratorInterfacePythia6Hadronisation" file="Pythia6Hadronisation.cc">
	<use name="pythia6"/>
	<flags EDM_PLUGIN="1"/>
</library>
<library name="GeneratorInterfacePythia8Hadronisation" file="Pythia8Hadronisation.cc">
	<use name="pythia8"/>
	<flags EDM_PLUGIN="1"/>
</library>
<library name="GeneratorInterfaceHerwig6Hadronisation" file="Herwig6Hadronisation.cc hwudat.f hwmsct_dummy.F pathhelper.F cms_hwwarn.F">
	<use name="f77compiler"/>
	<flags LDFLAGS="-lherwig -lherwig_dummy -llhapdf -llhapdf_dummy -ljimmy"/>
	<flags EDM_PLUGIN="1"/>
</library>
