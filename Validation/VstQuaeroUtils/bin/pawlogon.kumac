opt tic
opt zfl1
set stat 111110
set fit 111
graphics/set csiz 0.4
opt nbox
set hwid 4
set ywid 4
set xwin 3
set xwid 4
set bwid 4
set fwid 4
set vsiz 0.35
set tsiz 0.5
set asiz 0.5
set xval .6
set xlab 2.1
set xmgl 3.0
set pass 3
set cshi .03
set csiz 0.28
set pwid 1.
igset lwid 2.
igset mtyp 20
igset mscf .2
DEFAULTS -AUTO
OPT STAT
alias/create jkey   'exec ~/percent#percent_key'
alias/create jtext  'exec ~/percent#percent_text'
alias/create jline  'exec ~/percent#percent_line'
alias/create jitx   'exec ~/percent#percent_itx'
alias/create jbox   'exec ~/percent#percent_box'
alias/create jarrow 'exec ~/percent#percent_arrow'
alias/create jhelp  'exec ~/percent#HELP'
alias/create save_pic 'exec ~/save_picture' 
alias/create save_eps 'exec ~/save_picture#save_eps' 
alias/create no_save_pic 'V/CREATE SAVE_ME(1) R 1' 


filecase keep
