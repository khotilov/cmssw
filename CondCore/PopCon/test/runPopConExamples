#run popcon example1
echo "populate 10,20,30, then 5?,15?,25?, then 50,60,70"
cmsRun $LOCALRT/src/CondCore/PopCon/test/PopConExample1.cfg
echo "inspect db"
cmscond_list_iov -c sqlite_file:pop_test.db
echo "get iov details"
cmscond_list_iov -c sqlite_file:pop_test.db -t Example_tag1
echo "dump log"
cmscond_logdb_dump -c sqlite_file:log.db  

 
