#run Efficiency writer and reader
mkdir EffTest
pushd EffTest

cp $LOCALRT/src/CondCore/PopCon/test/PopConEffExample.py PopConEffExample.py

foreach T (Test1 Test2 Test3 Test4)
  sed "s/TestN/$T/g" PopConEffExample.py > conf.py
  cmsRun conf.py
  rm conf.py
end

cmscond_list_iov -c sqlite_file:pop_test.db -t Example_tag1

cmsRun $LOCALRT/src/CondCore/PopCon/test/print_eff_example.py

popd




