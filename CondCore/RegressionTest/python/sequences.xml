<?xml version="1.0" encoding="ascii"?>
<xml>
	<test name="ORA_regression">
		<init>
			<command exec ="$TTEST/setupDB -c -A $TAUTH -C $TCORALCONNSTR" env ="ref"></command>
			<command exec ="$TTEST/testCompat -w ref -A $TAUTH -C $TCORALCONNSTR" env ="ref"></command>
		</init>
		<sequence>
			<command exec ="$TTEST/testCompat -R -A $TAUTH -C $TCORALCONNSTR" Rname = "Cand read Ref" env ="cand"></command>
			<command exec ="$TTEST/testCompat -w cand -A $TAUTH -C $TCORALCONNSTR" Rname = "Candidate write" env ="cand"></command>
			<command exec ="$TTEST/testCompat -R -A $TAUTH -C $TCORALCONNSTR" Rname = "Cand read Cand" env ="cand"></command>
			<command exec ="$TTEST/testCompat -R -A $TAUTH -C $TCORALCONNSTR" Rname = "Ref read Cand" env ="ref"></command>
		</sequence>
		<final>
			<command exec ="$TTEST/setupDB -D -A $TAUTH -C $TCORALCONNSTR" env ="ref"></command>
		</final>
	</test>
</xml>