<?xml version="1.0"?>
<DDDefinition xmlns="http://www.cern.ch/cms/DDL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.cern.ch/cms/DDL ../../DetectorDescription/Schema/DDLSchema.xsd">

<ConstantsSection label="tibmodule0.xml" eval="true">
 <Constant name="ShiftX"     value="6.3335*mm"/>
 <Constant name="HybridY1"   value="0.086*cm"/>
 <Constant name="HybridY2"   value="-0.056*cm"/>
 <Constant name="PA2Dz1"     value="0.300*cm"/>
 <Constant name="PA2Dz2"     value="0.600*cm"/>
 <Constant name="PA2Theta"   value="-2.86241*deg"/>
 <Constant name="PAY1"       value="0.086*cm"/>
 <Constant name="PAY2"       value="-0.056*cm"/>
 <Constant name="SideRailY1" value="-0.036*cm"/>
 <Constant name="SideRailY2" value="0.066*cm"/>
 <Constant name="LedgeDz1"   value="0.500*cm"/>
 <Constant name="LedgeDz2"   value="0.225*cm"/>
 <Constant name="LedgeTheta" value="-2.62422*deg"/>
 <Constant name="LedgeY"     value="-0.091*cm"/>
 <Constant name="LedgeZ"     value="7.7875*cm"/>
 <Constant name="Spacer1Dx"  value="3.000*cm"/>
 <Constant name="Spacer2Dx"  value="0.725*cm"/>
 <Constant name="Spacer1Dy"  value="0.051*cm"/>
 <Constant name="Spacer2Dy"  value="0.015*cm"/>
 <Constant name="Spacer1Dz"  value="0.450*cm"/>
 <Constant name="Spacer2Dz"  value="0.400*cm"/>
 <Constant name="Spacer3Dz"  value="0.925*cm"/>
 <Constant name="Spacer1Y"   value="0.051*cm"/>
 <Constant name="Spacer1Z"   value="7.700*cm"/>
 <Constant name="Spacer2Y"   value="0.015*cm"/>
 <Constant name="Spacer2Z"   value="5.900*cm"/>
 <Constant name="Spacer3X"   value="2.275*cm"/>
 <Constant name="Spacer3Z"   value="7.225*cm"/>
 <Constant name="ModCoolY"   value="-0.127*cm"/>
 <Constant name="WaferY1"    value="0.1260*cm"/>
 <Constant name="WaferY2"    value="-0.0960*cm"/>
 <Constant name="WaferZ"     value="1.23720*cm"/>
</ConstantsSection>

<SolidSection label="tibmodule0.xml">
 <Trapezoid name="TIBPA2"    dz="[tibmodpar:PADx]"    alp1="0*deg"   
                             bl1="[PA2Dz1]"           tl1="[PA2Dz1]" 
                             h1="[tibmodpar:PADy]"    alp2="0*deg" 
                             bl2="[PA2Dz2]"           tl2="[PA2Dz2]" 
                             h2="[tibmodpar:PADy]"    phi="0*deg"
                             theta="[PA2Theta]" />
 <Trapezoid name="TIBLedge0" dz="[tibmodpar:LedgeDx]" alp1="0*deg"   
                             bl1="[LedgeDz1]"         tl1="[LedgeDz1]"
                             h1="[tibmodpar:LedgeDy]" alp2="0*deg" 
                             bl2="[LedgeDz2]"         tl2="[LedgeDz2]"
                             h2="[tibmodpar:LedgeDy]" phi="0*deg"
                             theta="[LedgeTheta]" />
 <Box name="TIBSpacerBot"    dx="[Spacer1Dx]"         dy="[Spacer1Dy]"
                             dz="[Spacer1Dz]"/>
 <Box name="TIBSpacerTop"    dx="[Spacer1Dx]"         dy="[Spacer2Dy]"
                             dz="[Spacer2Dz]"/>
 <Box name="TIBSpacerSide"   dx="[Spacer2Dx]"         dy="[Spacer2Dy]"
                             dz="[Spacer3Dz]"/>
</SolidSection>

<LogicalPartSection label="tibmodule0.xml">
 <LogicalPart name="TIBHybrid0" category="unspecified">
  <rSolid name="tibmodpar:TIBHybrid"/>
  <rMaterial name="tibmaterial:TIB_ele1"/>
 </LogicalPart>
 <LogicalPart name="TIBPA2" category="unspecified">
  <rSolid name="tibmodule0:TIBPA2"/>
  <rMaterial name="tibmaterial:TIB_PA_ster"/>
 </LogicalPart>
 <LogicalPart name="TIBLedge0" category="unspecified">
  <rSolid name="tibmodule0:TIBLedge0"/>
  <rMaterial name="tibmaterial:TIB_ledge_bds"/>
 </LogicalPart>
 <LogicalPart name="TIBSpacerBot" category="unspecified">
  <rSolid name="tibmodule0:TIBSpacerBot"/>
  <rMaterial name="tibmaterial:TIB_mod_spacer2"/>
 </LogicalPart>
 <LogicalPart name="TIBSpacerTop" category="unspecified">
  <rSolid name="tibmodule0:TIBSpacerTop"/>
  <rMaterial name="tibmaterial:TIB_mod_spacer1"/>
 </LogicalPart>
 <LogicalPart name="TIBSpacerSide" category="unspecified">
  <rSolid name="tibmodule0:TIBSpacerSide"/>
  <rMaterial name="tibmaterial:TIB_mod_spacer1"/>
 </LogicalPart>
 <LogicalPart name="TIBWaferRphi0" category="unspecified">
  <rSolid name="tibmodpar:TIBWafer"/>
  <rMaterial name="materials:Silicon"/>
 </LogicalPart>
 <LogicalPart name="TIBActiveRphi0" category="unspecified">
  <rSolid name="tibmodpar:TIBActive"/>
  <rMaterial name="materials:Silicon"/>
 </LogicalPart>
 <LogicalPart name="TIBWaferSter0" category="unspecified">
  <rSolid name="tibmodpar:TIBWafer"/>
  <rMaterial name="materials:Silicon"/>
 </LogicalPart>
 <LogicalPart name="TIBActiveSter0" category="unspecified">
  <rSolid name="tibmodpar:TIBActive"/>
  <rMaterial name="materials:Silicon"/>
 </LogicalPart>
</LogicalPartSection>

<PosPartSection label="tibmodule0.xml">
 <PosPart copyNumber="1">
   <rParent name="tibmodule0:TIBWaferRphi0"/>
   <rChild name="tibmodule0:TIBActiveRphi0"/>
 </PosPart>
 <PosPart copyNumber="1">
   <rParent name="tibmodule0:TIBWaferSter0"/>
   <rChild name="tibmodule0:TIBActiveSter0"/>
 </PosPart>
</PosPartSection>

</DDDefinition>
