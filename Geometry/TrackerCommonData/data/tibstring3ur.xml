<?xml version="1.0"?>
<DDDefinition xmlns="http://www.cern.ch/cms/DDL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.cern.ch/cms/DDL ../../../DetectorDescription/Schema/DDLSchema.xsd">

<ConstantsSection label="tibstring3ur.xml" eval="true">
 <Constant name="zero"         value="0.0*fm"/>
 <Constant name="MotherCableL" value="67.60*cm"/>
 <Constant name="CoolL"        value="68.00*cm"/>
 <Constant name="Det0Z"        value="149.865*mm"/>
 <Constant name="Det1Z"        value="377.804*mm"/>
 <Constant name="Det2Z"        value="605.456*mm"/>
 <Constant name="ICC0Z"        value="2.480*cm"/>
 <Constant name="ICC1Z"        value="25.170*cm"/>
 <Constant name="ICC2Z"        value="47.950*cm"/>
 <Constant name="ShiftDet"     value="-[tibstring3c:StringUpMinL]/2+[tibstring3c:WaferZUpOffset]"/>
 <Constant name="ShiftCCU"     value="-[tibstring3c:StringUpMinL]/2"/>
 <Constant name="ShiftICC"     value="[ShiftDet]+[tibstring3c:ICCUpOffset]"/>
</ConstantsSection>

<SolidSection label="tibstring3ur.xml">
 <Box name="TIBString3UpPls1"            dx="[tibmodpar:ModuleDx]"    
      dy="[tibmodpar:SSModuleDy]"          dz="[tibstring3c:StringUpPlsL]/2"/>
 <Box name="TIBString3UpPls2"            dx="[tibstringpar:CoolTubeW]/2"    
      dy="[tibstringpar:CoolTubeD]/2"    dz="[tibstring3c:StringUpPlsL]/2"/>
 <Box name="TIBString3UpPlsCable"        dx="[tibstringpar:MotherCableW]/2"    
      dy="[tibstringpar:MotherCableT]/2" dz="[MotherCableL]/2"/>
 <Tubs name="TIBString3UpPlsCoolS"       rMin="[zero]"      
       rMax="[tibstringpar:CoolTubeD]/2" dz="[CoolL]/2"
       startPhi="0*deg"                  deltaPhi="360*deg"/>
 <Tubs name="TIBString3UpPlsCoolW"       rMin="[zero]"      
       rMax="[tibstringpar:CoolTubeD]/2" dz="[tibstringpar:CoolTubeW]/2"
       startPhi="0*deg"                  deltaPhi="360*deg"/>
 </SolidSection>

<LogicalPartSection label="tibstring3ur.xml">
 <LogicalPart name="TIBString3UpPls1" category="unspecified">
  <rSolid name="TIBString3UpPls1"/>
  <rMaterial name="materials:Air"/>
 </LogicalPart>
 <LogicalPart name="TIBString3UpPls2" category="unspecified">
  <rSolid name="TIBString3UpPls2"/>
  <rMaterial name="materials:Air"/>
 </LogicalPart>
 <LogicalPart name="TIBString3UpPlsCable" category="unspecified">
  <rSolid name="TIBString3UpPlsCable"/>
  <rMaterial name="tibmaterial:TIB_mother_cable"/>
 </LogicalPart>
 <LogicalPart name="TIBString3UpPlsCoolS" category="unspecified">
  <rSolid name="TIBString3UpPlsCoolS"/>
  <rMaterial name="tibmaterial:TIB_mod_cool"/>
 </LogicalPart>
 <LogicalPart name="TIBString3UpPlsCoolW" category="unspecified">
  <rSolid name="TIBString3UpPlsCoolW"/>
  <rMaterial name="tibmaterial:TIB_mod_cool"/>
 </LogicalPart>
</LogicalPartSection>

<PosPartSection label="tibstring3ur.xml">
 <PosPart copyNumber="1">
   <rParent name="tibstring3ur:TIBString3UpPls1"/>
   <rChild name="tibstringpar:TIBCCUM"/>
  <Translation x="[zero]"   y="[zero]"  z="([tibstringpar:CCUMZ]+[ShiftCCU])" />
 </PosPart>
 <PosPart copyNumber="1">
   <rParent name="tibstring3ur:TIBString3UpPls2"/>
   <rChild name="tibstring3ur:TIBString3UpPlsCable"/>
  <Translation x="[zero]"   y="-[tibstringpar:MotherCableY]"  
               z="([tibstring3c:StringUpPlsL]-[MotherCableL])/2" />
 </PosPart>
 <PosPart copyNumber="1">
   <rParent name="tibstring3ur:TIBString3UpPls2"/>
   <rChild name="tibstring3ur:TIBString3UpPlsCoolS"/>
  <Translation x="-[tibstringpar:CoolTubeSep]/2"   y="[zero]"  
               z="([tibstring3c:StringUpPlsL]-[CoolL])/2" />
 </PosPart>
 <PosPart copyNumber="2">
   <rParent name="tibstring3ur:TIBString3UpPls2"/>
   <rChild name="tibstring3ur:TIBString3UpPlsCoolS"/>
  <Translation x="[tibstringpar:CoolTubeSep]/2"    y="[zero]"  
               z="([tibstring3c:StringUpPlsL]-[CoolL])/2" />
 </PosPart>
 <PosPart copyNumber="1">
   <rParent name="tibstring3ur:TIBString3UpPls2"/>
   <rChild name="tibstring3ur:TIBString3UpPlsCoolW"/>
  <rRotation name="tibstringpar:90XD"/>
  <Translation x="[zero]"   y="[zero]"  z="-([CoolL]-
               ([tibstring3c:StringUpPlsL]-[tibstringpar:CoolTubeD])/2)" />
 </PosPart>
</PosPartSection>

<Algorithm name="track:DDTrackerZPosAlgo">
  <rParent name="tibstring3ur:TIBString3UpPls1"/>
  <String name="ChildName"    value="tibmodule2:TIBModule2"/>
  <Numeric name="StartCopyNo" value="4"/>
  <Numeric name="IncrCopyNo"  value="1" />
  <Vector name="ZPositions" type="numeric" nEntries="3">
  ([Det0Z]+[ShiftDet]), ([Det1Z]+[ShiftDet]), ([Det2Z]+[ShiftDet]) </Vector>
  <Vector name="Rotations" type="string" nEntries="3">
  tibstringpar:NULL, tibstringpar:NULL, tibstringpar:NULL</Vector>
</Algorithm>

<Algorithm name="track:DDTrackerZPosAlgo">
  <rParent name="tibstring3ur:TIBString3UpPls1"/>
  <String name="ChildName"    value="tibstringss:TIBICCBoxSS"/>
  <Numeric name="StartCopyNo" value="5"/>
  <Numeric name="IncrCopyNo"  value="1" />
  <Vector name="ZPositions" type="numeric" nEntries="3">
  ([Det0Z]+[ShiftICC]), ([Det1Z]+[ShiftICC]), ([Det2Z]+[ShiftICC]) </Vector>
   <Vector name="Rotations" type="string" nEntries="3">
  tibstringpar:180D, tibstringpar:180D, tibstringpar:180D</Vector>
</Algorithm>
 
</DDDefinition>
