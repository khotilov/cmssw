<?xml version="1.0"?>
<DDDefinition xmlns="http://www.cern.ch/cms/DDL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.cern.ch/cms/DDL ../../DetectorDescription/Schema/DDLSchema.xsd">

<ConstantsSection label="tibstring1ul.xml" eval="true">
 <Constant name="zero"         value="0.0*fm"/>
 <Constant name="MotherCableL" value="52.20*cm"/>
 <Constant name="CoolL"        value="68.90*cm"/>
 <Constant name="Det0Z"        value="-546.373*mm"/>
 <Constant name="Det1Z"        value="-315.364*mm"/>
 <Constant name="Det2Z"        value="-83.6250*mm"/>
 <Constant name="ICC0Z"        value="-65.950*cm"/>
 <Constant name="ICC1Z"        value="-44.290*cm"/>
 <Constant name="ICC2Z"        value="-21.150*cm"/>
 <Constant name="ShiftDet"     value="[tibstring1c:StringUpPlsL]/2+[tibstring1c:WaferZUpOffset]"/>
 <Constant name="ShiftCCU"     value="[tibstring1c:StringUpPlsL]/2"/>
 <Constant name="ShiftICC"     value="[ShiftDet]+[tibstring1c:ICCUpOffset]"/>
</ConstantsSection>

<SolidSection label="tibstring1ul.xml">

 <Box name="TIBString1UpMin1MainPart"            dx="[tibmodpar:ModuleDx]"    
      dy="[tibmodpar:DSModuleDy]"   dz="[tibstring1c:StringUpMinL]/2"/>
 <Box name="TIBString1UpMin1SideCutL"            dx="[tibmodpar:DSModuleSideCutDx]"    
      dy="[tibmodpar:DSModuleSideCutDy]"   dz="[tibstring1c:StringUpMinL]/2+[tibmodpar:SideCutExtraZ]"/>
 <Box name="TIBString1UpMin1SideCutR"            dx="[tibmodpar:DSModuleSideCutDx]"    
      dy="[tibmodpar:DSModuleSideCutDy]"   dz="[tibstring1c:StringUpMinL]/2+[tibmodpar:SideCutExtraZ]"/>
 <SubtractionSolid name="TIBString1UpMin1LeftCutted">
   <rSolid name="TIBString1UpMin1MainPart"/>
   <rSolid name="TIBString1UpMin1SideCutL"/>
   <Translation x="-[tibmodpar:DSModuleSideCutX]" y="[tibmodpar:DSModuleSideCutY]" z="0."/>
 </SubtractionSolid>
 <SubtractionSolid name="TIBString1UpMin1">
   <rSolid name="TIBString1UpMin1LeftCutted"/>
   <rSolid name="TIBString1UpMin1SideCutR"/>
   <Translation x="[tibmodpar:DSModuleSideCutX]" y="[tibmodpar:DSModuleSideCutY]" z="0."/>
 </SubtractionSolid>

 <Box name="TIBString1UpMin2"            dx="[tibstringpar:CoolTubeW]/2"    
      dy="[tibstringpar:CoolTubeD]/2"    dz="[tibstring1c:StringUpMinL]/2"/>
 <Box name="TIBString1UpMinCable"        dx="[tibstringpar:MotherCableW]/2"    
      dy="[tibstringpar:MotherCableT]/2" dz="[MotherCableL]/2"/>
 <Tubs name="TIBString1UpMinCoolS"       rMin="[zero]"      
       rMax="[tibstringpar:CoolTubeD]/2" dz="[CoolL]/2"
       startPhi="0*deg"                  deltaPhi="360*deg"/>
 <Tubs name="TIBString1UpMinCoolW"       rMin="[zero]"      
       rMax="[tibstringpar:CoolTubeD]/2" dz="[tibstringpar:CoolTubeW]/2"
       startPhi="0*deg"                  deltaPhi="360*deg"/>
 </SolidSection>

<LogicalPartSection label="tibstring1ul.xml">
 <LogicalPart name="TIBString1UpMin1" category="unspecified">
  <rSolid name="TIBString1UpMin1"/>
  <rMaterial name="materials:Air"/>
 </LogicalPart>
 <LogicalPart name="TIBString1UpMin2" category="unspecified">
  <rSolid name="TIBString1UpMin2"/>
  <rMaterial name="materials:Air"/>
 </LogicalPart>
 <LogicalPart name="TIBString1UpMinCable" category="unspecified">
  <rSolid name="TIBString1UpMinCable"/>
  <rMaterial name="tibmaterial:TIB_mother_cable"/>
 </LogicalPart>
 <LogicalPart name="TIBString1UpMinCoolS" category="unspecified">
  <rSolid name="TIBString1UpMinCoolS"/>
  <rMaterial name="tibmaterial:TIB_mod_cool"/>
 </LogicalPart>
 <LogicalPart name="TIBString1UpMinCoolW" category="unspecified">
  <rSolid name="TIBString1UpMinCoolW"/>
  <rMaterial name="tibmaterial:TIB_mod_cool"/>
 </LogicalPart>
</LogicalPartSection>

<PosPartSection label="tibstring1ul.xml">
 <PosPart copyNumber="8">
   <rParent name="tibstring1ul:TIBString1UpMin1"/>
   <rChild name="tibstringpar:TIBCCUM"/>
  <Translation x="[zero]"   y="[zero]"  z="(-[tibstringpar:CCUMZ]+[ShiftCCU])" />
 </PosPart>
 <PosPart copyNumber="1">
   <rParent name="tibstring1ul:TIBString1UpMin2"/>
   <rChild name="tibstring1ul:TIBString1UpMinCable"/>
  <Translation x="[zero]"   y="-[tibstringpar:MotherCableY]"  
               z="-([tibstring1c:StringUpMinL]-[MotherCableL])/2" />
 </PosPart>
 <PosPart copyNumber="1">
   <rParent name="tibstring1ul:TIBString1UpMin2"/>
   <rChild name="tibstring1ul:TIBString1UpMinCoolS"/>
  <Translation x="-[tibstringpar:CoolTubeSep]/2"   y="[zero]"  
               z="-([tibstring1c:StringUpMinL]-[CoolL])/2" />
 </PosPart>
 <PosPart copyNumber="2">
   <rParent name="tibstring1ul:TIBString1UpMin2"/>
   <rChild name="tibstring1ul:TIBString1UpMinCoolS"/>
  <Translation x="[tibstringpar:CoolTubeSep]/2"    y="[zero]"  
               z="-([tibstring1c:StringUpMinL]-[CoolL])/2" />
 </PosPart>
 <PosPart copyNumber="1">
   <rParent name="tibstring1ul:TIBString1UpMin2"/>
   <rChild name="tibstring1ul:TIBString1UpMinCoolW"/>
  <rRotation name="tibstringpar:90XD"/>
  <Translation x="[zero]"   y="[zero]"  z="([CoolL]-
               ([tibstring1c:StringUpMinL]-[tibstringpar:CoolTubeD])/2)" />
 </PosPart>
</PosPartSection>

<Algorithm name="track:DDTrackerZPosAlgo">
  <rParent name="tibstring1ul:TIBString1UpMin1"/>
  <String name="ChildName"    value="tibmodule0b:TIBModule0B"/>
  <Numeric name="StartCopyNo" value="1"/>
  <Numeric name="IncrCopyNo"  value="1" />
  <Vector name="ZPositions" type="numeric" nEntries="3">
  ([Det0Z]+[ShiftDet]), ([Det1Z]+[ShiftDet]), ([Det2Z]+[ShiftDet]) </Vector>
  <Vector name="Rotations" type="string" nEntries="3">
  tibstringpar:NULL, tibstringpar:NULL, tibstringpar:NULL</Vector>
</Algorithm>

<Algorithm name="track:DDTrackerZPosAlgo">
  <rParent name="tibstring1ul:TIBString1UpMin1"/>
  <String name="ChildName"    value="tibstringds:TIBICCBoxDS"/>
  <Numeric name="StartCopyNo" value="2"/>
  <Numeric name="IncrCopyNo"  value="1" />
  <Vector name="ZPositions" type="numeric" nEntries="3">
  ([Det0Z]+[ShiftICC]), ([Det1Z]+[ShiftICC]), ([Det2Z]+[ShiftICC]) </Vector>
   <Vector name="Rotations" type="string" nEntries="3">
  tibstringpar:NULL, tibstringpar:NULL, tibstringpar:NULL</Vector>
</Algorithm>
 
</DDDefinition>
