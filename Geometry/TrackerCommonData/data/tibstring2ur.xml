<?xml version="1.0"?>
<DDDefinition xmlns="http://www.cern.ch/cms/DDL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.cern.ch/cms/DDL ../../../DetectorDescription/Schema/DDLSchema.xsd">

<ConstantsSection label="tibstring2ur.xml" eval="true">
 <Constant name="zero"         value="0.0*fm"/>
 <Constant name="MotherCableL" value="53.10*cm"/>
 <Constant name="CoolL"        value="69.70*cm"/>
 <Constant name="Det0Z"        value="74.5760*mm"/>
 <Constant name="Det1Z"        value="301.441*mm"/>
 <Constant name="Det2Z"        value="527.901*mm"/>
 <Constant name="ICC0Z"        value="20.010*cm"/>
 <Constant name="ICC1Z"        value="42.670*cm"/>
 <Constant name="ICC2Z"        value="65.020*cm"/>
 <Constant name="ShiftDet"     value="-[tibstring2c:StringUpMinL]/2+[tibstring2c:WaferZUpOffset]"/>
 <Constant name="ShiftCCU"     value="-[tibstring2c:StringUpMinL]/2"/>
 <Constant name="ShiftICC"     value="[ShiftDet]+[tibstring2c:ICCUpOffset]"/>
</ConstantsSection>

<SolidSection label="tibstring2ur.xml">
 <Box name="TIBString2UpPls1"            dx="[tibmodpar:ModuleDx]"    
      dy="[tibmodpar:SSModuleDy]"          dz="[tibstring2c:StringUpPlsL]/2"/>
 <Box name="TIBString2UpPls2"            dx="[tibstringpar:CoolTubeW]/2"    
      dy="[tibstringpar:CoolTubeD]/2"    dz="[tibstring2c:StringUpPlsL]/2"/>
 <Box name="TIBString2UpPlsCable"        dx="[tibstringpar:MotherCableW]/2"    
      dy="[tibstringpar:MotherCableT]/2" dz="[MotherCableL]/2"/>
 <Tubs name="TIBString2UpPlsCoolS"       rMin="[zero]"      
       rMax="[tibstringpar:CoolTubeD]/2" dz="[CoolL]/2"
       startPhi="0*deg"                  deltaPhi="360*deg"/>
 <Tubs name="TIBString2UpPlsCoolW"       rMin="[zero]"      
       rMax="[tibstringpar:CoolTubeD]/2" dz="[tibstringpar:CoolTubeW]/2"
       startPhi="0*deg"                  deltaPhi="360*deg"/>
 </SolidSection>

<LogicalPartSection label="tibstring2ur.xml">
 <LogicalPart name="TIBString2UpPls1" category="unspecified">
  <rSolid name="TIBString2UpPls1"/>
  <rMaterial name="materials:Air"/>
 </LogicalPart>
 <LogicalPart name="TIBString2UpPls2" category="unspecified">
  <rSolid name="TIBString2UpPls2"/>
  <rMaterial name="materials:Air"/>
 </LogicalPart>
 <LogicalPart name="TIBString2UpPlsCable" category="unspecified">
  <rSolid name="TIBString2UpPlsCable"/>
  <rMaterial name="tibmaterial:TIB_mother_cable"/>
 </LogicalPart>
 <LogicalPart name="TIBString2UpPlsCoolS" category="unspecified">
  <rSolid name="TIBString2UpPlsCoolS"/>
  <rMaterial name="tibmaterial:TIB_mod_cool"/>
 </LogicalPart>
 <LogicalPart name="TIBString2UpPlsCoolW" category="unspecified">
  <rSolid name="TIBString2UpPlsCoolW"/>
  <rMaterial name="tibmaterial:TIB_mod_cool"/>
 </LogicalPart>
</LogicalPartSection>

<PosPartSection label="tibstring2ur.xml">
 <PosPart copyNumber="1">
   <rParent name="tibstring2ur:TIBString2UpPls1"/>
   <rChild name="tibstringpar:TIBCCUM"/>
  <Translation x="[zero]"   y="[zero]"  z="([tibstringpar:CCUMZ]+[ShiftCCU])" />
 </PosPart>
 <PosPart copyNumber="1">
   <rParent name="tibstring2ur:TIBString2UpPls2"/>
   <rChild name="tibstring2ur:TIBString2UpPlsCable"/>
  <Translation x="[zero]"   y="-[tibstringpar:MotherCableY]"  
               z="([tibstring2c:StringUpPlsL]-[MotherCableL])/2" />
 </PosPart>
 <PosPart copyNumber="1">
   <rParent name="tibstring2ur:TIBString2UpPls2"/>
   <rChild name="tibstring2ur:TIBString2UpPlsCoolS"/>
  <Translation x="-[tibstringpar:CoolTubeSep]/2"   y="[zero]"  
               z="([tibstring2c:StringUpPlsL]-[CoolL])/2" />
 </PosPart>
 <PosPart copyNumber="2">
   <rParent name="tibstring2ur:TIBString2UpPls2"/>
   <rChild name="tibstring2ur:TIBString2UpPlsCoolS"/>
  <Translation x="[tibstringpar:CoolTubeSep]/2"    y="[zero]"  
               z="([tibstring2c:StringUpPlsL]-[CoolL])/2" />
 </PosPart>
 <PosPart copyNumber="1">
   <rParent name="tibstring2ur:TIBString2UpPls2"/>
   <rChild name="tibstring2ur:TIBString2UpPlsCoolW"/>
  <rRotation name="tibstringpar:90XD"/>
  <Translation x="[zero]"   y="[zero]"  z="-([CoolL]-
               ([tibstring2c:StringUpPlsL]-[tibstringpar:CoolTubeD])/2)" />
 </PosPart>
</PosPartSection>

<Algorithm name="track:DDTrackerZPosAlgo">
  <rParent name="tibstring2ur:TIBString2UpPls1"/>
  <String name="ChildName"    value="tibmodule2:TIBModule2"/>
  <Numeric name="StartCopyNo" value="4"/>
  <Numeric name="IncrCopyNo"  value="1" />
  <Vector name="ZPositions" type="numeric" nEntries="3">
  ([Det0Z]+[ShiftDet]), ([Det1Z]+[ShiftDet]), ([Det2Z]+[ShiftDet]) </Vector>
  <Vector name="Rotations" type="string" nEntries="3">
  tibstringpar:180D, tibstringpar:180D, tibstringpar:180D</Vector>
</Algorithm>

<Algorithm name="track:DDTrackerZPosAlgo">
  <rParent name="tibstring2ur:TIBString2UpPls1"/>
  <String name="ChildName"    value="tibstringss:TIBICCBoxSS"/>
  <Numeric name="StartCopyNo" value="5"/>
  <Numeric name="IncrCopyNo"  value="1" />
  <Vector name="ZPositions" type="numeric" nEntries="3">
  ([Det0Z]+[ShiftICC]), ([Det1Z]+[ShiftICC]), ([Det2Z]+[ShiftICC]) </Vector>
  <Vector name="Rotations" type="string" nEntries="3">
  tibstringpar:NULL, tibstringpar:NULL, tibstringpar:NULL</Vector>
</Algorithm>
 
</DDDefinition>
