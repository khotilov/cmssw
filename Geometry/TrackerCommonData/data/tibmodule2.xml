<?xml version="1.0"?>
<DDDefinition xmlns="http://www.cern.ch/cms/DDL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.cern.ch/cms/DDL ../../../DetectorDescription/Schema/DDLSchema.xsd">

<ConstantsSection label="tibmodule2.xml" eval="true">
 <Constant name="zero"       value="0.0*fm"/>
 <Constant name="HybridY"    value="-0.041*cm"/>
 <Constant name="PAY"        value="-0.041*cm"/>
 <Constant name="SideRailY"  value="-[tibmodpar:SSModuleDy]+[tibmodpar:SideRailDy]"/>
 <Constant name="LedgeDz"    value="0.450*cm"/>
 <Constant name="LedgeY"     value="-0.086*cm"/>
 <Constant name="LedgeZ"     value="7.700*cm"/>
 <Constant name="ModCoolY"   value="-0.112*cm"/>
 <Constant name="WaferY"     value="-0.160*mm"/>
</ConstantsSection>

<SolidSection label="tibmodule2.xml">
 <Box name="TIBLedge2"       dx="[tibmodpar:LedgeDx]" dy="[tibmodpar:LedgeDy]"
                             dz="[LedgeDz]"/>
</SolidSection>

<LogicalPartSection label="tibmodule2.xml">
 <LogicalPart name="TIBModule2" category="unspecified">
  <rSolid name="tibmodpar:TIBSSModule"/>
  <rMaterial name="materials:Air"/>
 </LogicalPart>
 <LogicalPart name="TIBHybrid2" category="unspecified">
  <rSolid name="tibmodpar:TIBHybrid"/>
  <rMaterial name="tibtidcommonmaterial:TIBTID_ele3"/>
 </LogicalPart>
 <LogicalPart name="TIBLedge2" category="unspecified">
  <rSolid name="tibmodule2:TIBLedge2"/>
  <rMaterial name="tibmaterial:TIB_ledge_bss"/>
 </LogicalPart>
 <LogicalPart name="TIBWaferRphi2" category="unspecified">
  <rSolid name="tibmodpar:TIBSSWafer"/>
  <rMaterial name="materials:Silicon"/>
 </LogicalPart>
 <LogicalPart name="TIBActiveRphi2" category="unspecified">
  <rSolid name="tibmodpar:TIBSSActive"/>
  <rMaterial name="materials:Silicon"/>
 </LogicalPart>
</LogicalPartSection>

<PosPartSection label="tibmodule2.xml">
 <PosPart copyNumber="1">
   <rParent name="tibmodule2:TIBWaferRphi2"/>
   <rChild name="tibmodule2:TIBActiveRphi2"/>
  <Translation x="[zero]"  y="[zero]" 
               z="0.5*[tracker:BackPlaneDz]" />
 </PosPart>
 <PosPart copyNumber="1">
   <rParent name="tibmodule2:TIBModule2"/>
   <rChild name="tibmodule2:TIBWaferRphi2"/>
  <rRotation name="tibmodpar:Rphi"/>
  <Translation x="[zero]"                  y="[tibmodule2:WaferY]" 
               z="[tibmodpar:WaferZ]" />
 </PosPart>
 <PosPart copyNumber="1">
   <rParent name="tibmodule2:TIBModule2"/>
   <rChild name="tibmodule2:TIBHybrid2"/>
  <Translation x="[zero]"                  y="[tibmodule2:HybridY]" 
               z="-[tibmodpar:HybridZ]"  />
 </PosPart>
 <PosPart copyNumber="1">
   <rParent name="tibmodule2:TIBModule2"/>
   <rChild name="tibmodpar:TIBPA1"/>
  <Translation x="[zero]"                  y="[tibmodule2:PAY]" 
               z="-[tibmodpar:PAZ]"  />
 </PosPart>
 <PosPart copyNumber="1">
   <rParent name="tibmodule2:TIBModule2"/>
   <rChild name="tibmodpar:TIBSideRailL"/>
  <Translation x="[tibmodpar:SideRailX]"   y="[tibmodule2:SideRailY]" 
               z="[tibmodpar:SideRailZ]" />
 </PosPart>
 <PosPart copyNumber="1">
   <rParent name="tibmodule2:TIBModule2"/>
   <rChild name="tibmodpar:TIBSideRailR"/>
  <Translation x="-[tibmodpar:SideRailX]"  y="[tibmodule2:SideRailY]" 
               z="[tibmodpar:SideRailZ]" />
 </PosPart>
 <PosPart copyNumber="1">
   <rParent name="tibmodule2:TIBModule2"/>
   <rChild name="tibmodule2:TIBLedge2"/>
  <Translation x="[zero]"                  y="[tibmodule2:LedgeY]" 
               z="[tibmodule2:LedgeZ]"  />
 </PosPart>
 <PosPart copyNumber="1">
   <rParent name="tibmodule2:TIBModule2"/>
   <rChild name="tibmodpar:TIBModCoolTop"/>
  <Translation x="[tibmodpar:ModCool1X]"   y="[tibmodule2:ModCoolY]" 
               z="-[tibmodpar:ModCool1Z]" />
 </PosPart>
 <PosPart copyNumber="1">
   <rParent name="tibmodule2:TIBModule2"/>
   <rChild name="tibmodpar:TIBModCoolSide"/>
  <Translation x="-[tibmodpar:ModCool2X]"  y="[tibmodule2:ModCoolY]" 
               z="-[tibmodpar:ModCool2Z]" />
 </PosPart>
 <PosPart copyNumber="2">
   <rParent name="tibmodule2:TIBModule2"/>
   <rChild name="tibmodpar:TIBModCoolSide"/>
  <Translation x="[tibmodpar:ModCool2X]"   y="[tibmodule2:ModCoolY]" 
               z="-[tibmodpar:ModCool2Z]" />
 </PosPart>
</PosPartSection>

</DDDefinition>
