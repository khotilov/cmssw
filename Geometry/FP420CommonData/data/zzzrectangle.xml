<PosPartSection label="zzzrectangle.xml">
<!--  -->
<!--  -->
<!-- for 4 stations half z dimension of FP420E bigger 3.5 * [ZUnit]:    -->
<!-- ( 3.5 * 2500mm = 8750mm  ); now there dZ = 10.0*m - it is large enough   -->
 <PosPart copyNumber="1">
   <rParent name="fp420:FP420E"/>
   <rChild name="zzzrectangle:FP420Ex"/>
  <Translation x="0*fm" y="0*fm" z="0*fm" />
  <rRotation name="FP420Rot:FP420MAIN"/>
 </PosPart>
 <PosPart copyNumber="2">
   <rParent name="fp420:FP420E"/>
   <rChild name="zzzrectangle:FP420Ex"/>
  <Translation x="0*fm" y="0*fm" z="0*fm+[ZUnit]" />
  <rRotation name="FP420Rot:FP420MAIN"/>
 </PosPart>
 <PosPart copyNumber="3">
   <rParent name="fp420:FP420E"/>
   <rChild name="zzzrectangle:FP420Ex"/>
  <Translation x="0*fm" y="0*fm" z="0*fm+2*[ZUnit]" />
  <rRotation name="FP420Rot:FP420MAIN"/>
 </PosPart>
 <PosPart copyNumber="4">
   <rParent name="fp420:FP420E"/>
   <rChild name="zzzrectangle:FP420Ex"/>
  <Translation x="0*fm" y="0*fm" z="0*fm+3*[ZUnit]" />
  <rRotation name="FP420Rot:FP420MAIN"/>
 </PosPart>
<!--  -->
 <PosPart copyNumber="1">
   <rParent name="zzzrectangle:FP420Ex"/>
   <rChild name="zzzrectangle:SBST"/>
  <rRotation name="FP420Rot:FP420MAIN"/>
 </PosPart>
 <PosPart copyNumber="1">
   <rParent name="zzzrectangle:FP420Ex"/>
   <rChild name="zzzrectangle:FlIn"/>
  <Translation x="0*fm" y="0*fm" z="[TubZ]-[ZinFlanze]/2" />
  <rRotation name="FP420Rot:FP420MAIN"/>
 </PosPart>
 <PosPart copyNumber="2">
   <rParent name="zzzrectangle:FP420Ex"/>
   <rChild name="zzzrectangle:FlIn"/>
  <Translation x="0*fm" y="0*fm" z="-[TubZ]+[ZinFlanze]/2" />
  <rRotation name="FP420Rot:FP420MAIN"/>
 </PosPart>
 <PosPart copyNumber="1">
   <rParent name="zzzrectangle:FP420Ex"/>
