 <LogicalPart name="FP420Ex" category="unspecified">
  <rSolid name="FP420Ex"/>
  <rMaterial name="materialsfp420:Air"/>
 </LogicalPart>
<!--  -->
<!-- Pocket area: -->
<!--  -->
 <LogicalPart name="SBST" category="unspecified">
  <rSolid name="SBST"/>
