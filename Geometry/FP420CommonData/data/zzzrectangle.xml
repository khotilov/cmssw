<?xml version="1.0"?>
<DDDefinition xmlns="http://www.cern.ch/cms/DDL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.cern.ch/cms/DDL ../../DDLSchema/DDLSchema.xsd">
<!--    -->
 <ConstantsSection label="zzzrectangle.xml" eval="true">
<!--  -->
<!--  -->
<!-- DEFINE 3D IN Y DIRECTION; IT WILL BE ROTATED THEN IN 90 DEGREE TO LIE AS IN REALITY (IN X) -->
<!--  -->
<!-- external radius of beam pipe in pocket area  (it was 33.35 before)) -->
 <Constant name="TubR"          value="35.0*mm"/>
<!-- external radius of beam pipe out pocket area    -->
 <Constant name="OutTubR"          value="40.0*mm"/>
<!--  -->
<!--  -->
<!-- x-size of pocket:it's not possible to have BoxDx bigger TubR    -->
<!-- 2.* BoxDx(=15) must be bigger   XSi   -->
 <Constant name="BoxDx"         value="10.5*mm"/>
<!-- copper x-thickness of pocket  -->
 <Constant name="DxThickCop"    value="0.05*mm"/>
<!-- st.steel x-thickness of pocket  -->
 <Constant name="DxThickSte"    value="0.15*mm"/>
<!--z-size of flat part of pocket = 2*BoxDz (= 40.0*mm)  -->
 <Constant name="BoxDz"         value="20.0*mm"/>
                                                         <!--Copper -->
<!-- copper thickness for beam pipe Radius in pocket range -->
 <Constant name="dRcopper"         value="0.05*mm"/>
<!-- copper thickness (in Y) in range of flat pocket part -->
 <Constant name="dYcopper"         value="0.05*mm"/>
<!-- copper thickness of window                           -->
 <Constant name="dZcopper"         value="0.05*mm"/>
<!--  -->
                                                     <!--StainlessSteel  -->
<!-- StainlessSteel thickness of beam pipe in pocket region -->
 <Constant name="dRsteel"         value="2.00*mm"/>
<!-- StSteel thickness (in Y) in range of flat pocket part -->
 <Constant name="dYsteel"         value="0.15*mm"/>
<!-- StSteel thickness of window                          -->
 <Constant name="dZsteel"         value="0.15*mm"/>
<!--  -->
<!--  -->
<!-- technical/safety value to construct SubtractionSolid --> 
 <Constant name="delta"           value="0.01*mm"/>
<!--  -->
<!--  -->
<!--  -->
<!-- y-size from tube center to bottom of flat horizontal pocket part -->
 <Constant name="gap"      value="12.3*mm"/>
<!--  -->
<!--    y-distance from tube center to top of flat pocket part:  -->
<!-- it's used because of previous set up , re-calculated as:  -->
 <Constant name="BoxYshft" value="[gap]+[dYcopper]+[dYsteel]"/>
<!--                          +12.3+0.05+0.15 = 12.5     -->
<!--  -->
<!--  -->
<!--    Complete Unit size for tube with radius TubR: -->
<!--  -->
<!-- area 2*ZinWidth for flanze in frame of (TubR) internal pocket area  -->
 <Constant name="ZinWidth"           value="25.*mm"/>
<!-- external Flanze radius -->
 <Constant name="RinFlanze"          value="50.*mm"/>
<!-- z-size of Flanze -->
 <Constant name="ZinFlanze"          value="14.*mm"/>
<!--  -->
<!--  unit of tube of the same radius with one pocket include:     -->
<!--     pocket inclined area and area for flanze:  -->
<!-- area 1) 2*[BoxDz] + 2*[ZinWidth]  = 2*TubZ -->
<!--         2*( 25    +(  33.35-16.82113357428167003697)/0.2679492)+2.*20. = 213.37313509962582432065  -->
<!-- So, 2*TubZ = 1)   -->
 <Constant name="TubZ" value="[BoxDz]+[ZinWidth]"/>
<!--  -->
<!-- outside the pocket: -->
<!-- area 2) it's area of other flanze and bellows: FP420E2 -->
<!-- 2*ZoutWidth for flanze and bellow out of pocket area (for tube radius OutTubR) -->
<!-- total width of area 2)  -->
 <Constant name="ZoutWidth"            value="100.*mm"/>
<!-- other flantz width in area 2) -->
 <Constant name="ZoutFlanze"           value="20.*mm"/>
<!-- thickness of tube in area 2) , so, R = OutTubR-ZoutFlanze to OutTubR    -->
 <Constant name="RoutThick"            value="4.0*mm"/>
<!-- bellows shift from flanze   -->
 <Constant name="BellowsShift"            value="10.*mm"       />
<!-- total width of bellows   -->
 <Constant name="BellowsWidth"            value="2*([ZoutWidth]/2 - [ZoutFlanze])-[BellowsShift]"       />
<!-- <Constant name="BellowsWidth"            value="50.0*mm"/>  -->
<!-- StSteel thickness of bellows   -->
 <Constant name="BellowsT"            value="1.0*mm"/> 
<!--  -->
<!--  -->
<!-- total unit length in z - distanse between centers of any 2 Stations    2700.*mm  4000.*mm 8000.*mm  -->
 <Constant name="ZUnit"           value="2700.*mm"/>
<!--  -->
<!-- area 3): thickness of this tube the same as for 2): RoutThick    -->
<!-- 2*ZpureTube just tube (OutTubR) to complete space between pockets  -->
<!-- if we want 2500.mm between Stations centers do calculations:  -->
<!-- (2500.-(2.*213.37313509962582432065   + 2.*100.))/2. = 936.62686490037417567935 -->
<!--  <Constant name="ZpureTube"           value="936.62686490037417567935*mm"/> -->
 <Constant name="ZpureTube"           value="([ZUnit]-2*([TubZ]+[ZoutWidth]))/2."/>
<!--  -->
<!--  -->
<!-- So, total Unit dimension is 1) (2*TubZ) + 2) (2*ZoutWidth) + 3) (2*ZpureTube) = [ZUnit]  -->
<!-- 2.*213.37313509962582432065   + 2.*100.    +2.*936.62686490037417567935   =  2500.    -->
<!--  -->
<!-- With ZsafetyShift variable one can change global z-position of pocket -->
<!--  -->
<!-- to put pocket in the center of beam pipe unit:  -->
 <Constant name="ZsafetyShift" value="[TubZ]-[BoxDz]"/>
<!-- instead of arbitrary shift from right edge of beam pipe unit: -->
<!--      <Constant name="ZsafetyShift"  value="400*mm"/>     -->
<!--  -->
<!--  -->
<!--  -->
<!-- Parameter Values for station:  -->
<!--  -->
 <Constant name="XSi"               value="10.1*mm"/>
 <Constant name="XSiPlane"          value="10.0*mm"/>
 <Constant name="XSiDet"            value="10.0*mm"/>
<!--  -->
 <Constant name="YSi"               value="20.1*mm"/>
 <Constant name="YSiPlane"          value="20.0*mm"/>
 <Constant name="YSiDet"            value="20.0*mm"/>
<!-- gap between top of flat horizontal part of pocket and detector(YSi) bottom -->
 <Constant name="dYGap"             value="0.2*mm"/>
<!--  -->
<!-- z-size of solid to keep all planes together -->
 <Constant name="ZSi"               value="25.00*mm"/>
<!-- distanse between plane centers: if 10 planes * ZSiStep = or less ZSi -->
 <Constant name="ZSiStep"           value="2.400*mm"/>
<!-- z- dimension of one plane -->
 <Constant name="ZSiPlane"          value="2.000*mm"/>
<!-- plane containe the next content: -->
<!-- z-gap left at start of Si plate  -->
 <Constant name="ZGapLDet"           value="0.100*mm"/>
<!-- z-size of left Si plate  -->
 <Constant name="ZSiDetL"           value="0.250*mm"/>
<!-- z-size of glue material between plates       -->
 <Constant name="ZBoundDet"         value="0.020*mm"/>
<!--z-size of right Si plate  (it was 0.300 before) -->
 <Constant name="ZSiDetR"           value="0.250*mm"/>
<!-- z-size of ceramic plate in plane (it was 1.0,0.5,0.3 before)-->
 <Constant name="ZCeramDet"         value="0.5*mm"/>
<!--  -->
<!--  -->
<!--  -->
<!--  -->
<!-- Parameter Values for pitch of read-out in X and Y -->
<!--  -->
<!-- PitchX was 0.040 -->
 <Constant name="PitchX"               value="0.050*mm"/>
 <Constant name="PitchY"               value="0.050*mm"/>
<!--  -->
<!-- to lift up the station just use work value ~ 100mm -->
 <Constant name="work"              value="0.000*mm"/>
<!--  -->
 </ConstantsSection>
<!-- -->
<!-- -->
<!-- -->
<!-- -->
<!-- -->
<!-- -->
 <SolidSection label="zzzrectangle.xml">
<!-- -->
 <Tubs name="FP420Ex" rMin="[fp420:Rmin]" rMax="[fp420:HfpR]" dz="[ZUnit]/2" startPhi="0*deg" deltaPhi="360*deg"/>
<!-- -->
<!--for area 2) -->
<!-- -->
 <Tubs name="FlIn" rMin="[TubR]" rMax="[RinFlanze]" dz="[ZinFlanze]/2" startPhi="0*deg" deltaPhi="360*deg"/>
<!-- -->
 <Tubs name="FP420E2" rMin="[fp420:Rmin]" rMax="[RinFlanze]" dz="[ZoutWidth]/2" startPhi="0*deg" deltaPhi="360*deg"/>
<!-- -->
 <Tubs name="FlOut" rMin="[OutTubR]" rMax="[RinFlanze]" dz="[ZoutFlanze]/2" startPhi="0*deg" deltaPhi="360*deg"/>
 <Tubs name="FlOutIn" rMin="[TubR]" rMax="[OutTubR]-[RoutThick]" dz="[ZoutFlanze]/2" startPhi="0*deg" deltaPhi="360*deg"/>
 <Tubs name="EdgeOut" rMin="[fp420:Rmin]" rMax="[OutTubR]" dz="[ZoutWidth]/2" startPhi="0*deg" deltaPhi="360*deg"/>
 <Tubs name="EdgeIn" rMin="[fp420:Rmin]" rMax="[OutTubR]-[RoutThick]" dz="[ZoutWidth]/2" startPhi="0*deg" deltaPhi="360*deg"/> <Tubs name="BellowsIn" rMin="[OutTubR]-[RoutThick]" rMax="[OutTubR]" dz="[BellowsWidth]/2" startPhi="0*deg" deltaPhi="360*deg"/>
<!-- 10 part of Bellow; if you want 15, change 20 to 30 and add some lines to have at end 30.:30 -->
 <Polycone name="Bellows" startPhi="0*deg" deltaPhi="360*deg" >
 <ZSection z="-[BellowsWidth]/2+(0./20)*[BellowsWidth]" rMin="[OutTubR]" rMax="[OutTubR]+[BellowsT]" />
 <ZSection z="-[BellowsWidth]/2+(1./20)*[BellowsWidth]" rMin="[RinFlanze]-[BellowsT]" rMax="[RinFlanze]"/>
 <ZSection z="-[BellowsWidth]/2+(2./20)*[BellowsWidth]" rMin="[OutTubR]" rMax="[OutTubR]+[BellowsT]"/>
 <ZSection z="-[BellowsWidth]/2+(3./20)*[BellowsWidth]" rMin="[RinFlanze]-[BellowsT]" rMax="[RinFlanze]"/>
 <ZSection z="-[BellowsWidth]/2+(4./20)*[BellowsWidth]" rMin="[OutTubR]" rMax="[OutTubR]+[BellowsT]"/>
 <ZSection z="-[BellowsWidth]/2+(5./20)*[BellowsWidth]" rMin="[RinFlanze]-[BellowsT]" rMax="[RinFlanze]"/>
 <ZSection z="-[BellowsWidth]/2+(6./20)*[BellowsWidth]" rMin="[OutTubR]" rMax="[OutTubR]+[BellowsT]"/>
 <ZSection z="-[BellowsWidth]/2+(7./20)*[BellowsWidth]" rMin="[RinFlanze]-[BellowsT]" rMax="[RinFlanze]"/>
 <ZSection z="-[BellowsWidth]/2+(8./20)*[BellowsWidth]" rMin="[OutTubR]" rMax="[OutTubR]+[BellowsT]"/>
 <ZSection z="-[BellowsWidth]/2+(9./20)*[BellowsWidth]" rMin="[RinFlanze]-[BellowsT]" rMax="[RinFlanze]"/>
 <ZSection z="-[BellowsWidth]/2+(10./20)*[BellowsWidth]" rMin="[OutTubR]" rMax="[OutTubR]+[BellowsT]"/>
 <ZSection z="-[BellowsWidth]/2+(11./20)*[BellowsWidth]" rMin="[RinFlanze]-[BellowsT]" rMax="[RinFlanze]"/>
 <ZSection z="-[BellowsWidth]/2+(12./20)*[BellowsWidth]" rMin="[OutTubR]" rMax="[OutTubR]+[BellowsT]"/>
 <ZSection z="-[BellowsWidth]/2+(13./20)*[BellowsWidth]" rMin="[RinFlanze]-[BellowsT]" rMax="[RinFlanze]"/>
 <ZSection z="-[BellowsWidth]/2+(14./20)*[BellowsWidth]" rMin="[OutTubR]" rMax="[OutTubR]+[BellowsT]"/>
 <ZSection z="-[BellowsWidth]/2+(15./20)*[BellowsWidth]" rMin="[RinFlanze]-[BellowsT]" rMax="[RinFlanze]"/>
 <ZSection z="-[BellowsWidth]/2+(16./20)*[BellowsWidth]" rMin="[OutTubR]" rMax="[OutTubR]+[BellowsT]"/>
 <ZSection z="-[BellowsWidth]/2+(17./20)*[BellowsWidth]" rMin="[RinFlanze]-[BellowsT]" rMax="[RinFlanze]"/>
 <ZSection z="-[BellowsWidth]/2+(18./20)*[BellowsWidth]" rMin="[OutTubR]" rMax="[OutTubR]+[BellowsT]"/>
 <ZSection z="-[BellowsWidth]/2+(19./20)*[BellowsWidth]" rMin="[RinFlanze]-[BellowsT]" rMax="[RinFlanze]"/>
 <ZSection z="-[BellowsWidth]/2+(20./20)*[BellowsWidth]" rMin="[OutTubR]" rMax="[OutTubR]+[BellowsT]"/>
 </Polycone>
<!--for area 3) -->
<!--  -->
 <Tubs name="EdgeOut3" rMin="[fp420:Rmin]" rMax="[OutTubR]" dz="[ZpureTube]/2" startPhi="0*deg" deltaPhi="360*deg"/>
 <Tubs name="EdgeIn3" rMin="[fp420:Rmin]" rMax="[OutTubR]-[RoutThick]" dz="[ZpureTube]/2" startPhi="0*deg" deltaPhi="360*deg"/><!-- -->
<!-- -->
<!--for area 1) -->
<!-- SBST:BPST-BOST     Copper -->
<!--  -->
 <Tubs name="BPST" rMin="[fp420:Rmin]" rMax="[TubR]" dz="[TubZ]" startPhi="0*deg" deltaPhi="360*deg"/>
<!-- -->
 <Trd1 name="BOST" dx1="[BoxDx]" dx2="[BoxDx]" dz="([TubR]-[BoxYshft]+[delta])/2"                      dy1="[BoxDz]"                                         dy2="[BoxDz]"/>
<!-- -->
 <Box name="CURR" dx="[BoxDx]" dy="([TubR]-[gap])/2" dz="[BoxDz]"/>
<!-- -->
 <SubtractionSolid name="SBS1">
  <rSolid name="BPST"/>
  <rSolid name="BOST"/>
  <Translation x="0*fm" y="([TubR]+[BoxYshft]+[delta])/2" z="[TubZ]-[ZsafetyShift]-[BoxDz]"/>
  <rRotation name="FP420Rot:FP42090"/>
 </SubtractionSolid>
<!-- -->
 <SubtractionSolid name="SBST">
  <rSolid name="SBS1"/>
  <rSolid name="CURR"/>
  <Translation x="0*fm" y="([TubR]+[gap])/2." z="[TubZ]-[ZsafetyShift]-[BoxDz]"/>
  <rRotation name="FP420Rot:FP420MAIN"/>
 </SubtractionSolid>
<!-- -->
<!-- -->
<!-- -->
<!-- -->
<!-- -->
<!-- -->
<!-- SBSTs:BPSTs-BOSTs     StainlessSteel   -->
<!--  -->
 <Tubs name="BPSTs" rMin="[fp420:Rmin]" rMax="[TubR]-[dRcopper]" dz="([TubZ])" startPhi="0*deg" deltaPhi="360*deg"/>
<!-- -->
<Trd1 name="BOSTs" dx1="[BoxDx]+[DxThickCop]" dx2="[BoxDx]+[DxThickCop]" dz="([TubR]-[dRcopper]-[BoxYshft]+[dYcopper]+[delta])/2" dy1="[BoxDz]+[dZcopper]"       dy2="[BoxDz]"/>
<!-- -->
 <SubtractionSolid name="SBS1s">
  <rSolid name="BPSTs"/>
  <rSolid name="BOSTs"/>
  <Translation x="0*fm" y="([TubR]-[dRcopper]+[BoxYshft]-[dYcopper]+[delta])/2"           z="[TubZ]-[ZsafetyShift]-[BoxDz]" />
  <rRotation name="FP420Rot:FP42090"/>
 </SubtractionSolid>
<!-- -->
 <SubtractionSolid name="SBSTs">
  <rSolid name="SBS1s"/>
  <rSolid name="CURR"/>
  <Translation x="0*fm" y=" ([TubR]+[gap])/2-[dYcopper]" z="[TubZ]-[ZsafetyShift]-[BoxDz]"/>
  <rRotation name="FP420Rot:FP420MAIN"/>
 </SubtractionSolid>
<!-- -->
<!-- -->
<!-- -->
<!-- -->
<!-- -->
<!-- SBSTm:BPSTm-BOSTm     Vacuum -->
<!--  -->
 <Tubs name="BPSTm" rMin="[fp420:Rmin]" rMax="[TubR]-[dRsteel]-[dRcopper]" dz="([TubZ])" startPhi="0*deg" deltaPhi="360*deg"/>
<!-- -->
 <Trd1 name="BOSTm" dx1="[BoxDx]+[DxThickCop]+[DxThickSte]" dx2="[BoxDx]+[DxThickCop]+[DxThickSte]" dz="([TubR]-[dRsteel]-[dRcopper]-[BoxYshft]+[dYsteel]+[dYcopper]+[delta])/2" dy1="[BoxDz]+[dZsteel]+[dZcopper]"  dy2="[BoxDz]"/>
<!-- -->
 <SubtractionSolid name="SBS1m">
  <rSolid name="BPSTm"/>
  <rSolid name="BOSTm"/>
  <Translation x="0*fm" y="([TubR]-[dRsteel]-[dRcopper]+[BoxYshft]-[dYsteel]-[dYcopper]+[delta])/2" z="[TubZ]-[ZsafetyShift]-[BoxDz]" />
  <rRotation name="FP420Rot:FP42090"/>
 </SubtractionSolid>
<!-- -->
 <SubtractionSolid name="SBSTm">
  <rSolid name="SBS1m"/>
  <rSolid name="CURR"/>
  <Translation x="0*fm" y="( [TubR]+[gap])/2. - [dYsteel] - [dYcopper]" z="[TubZ]-[ZsafetyShift]-[BoxDz]"/>
  <rRotation name="FP420Rot:FP420MAIN"/>
 </SubtractionSolid>
<!-- -->
<!--  -->
<!-- Si Det.:SISTATION:SIPLANE:(SIDETL+BOUNDDET        +SIDETR + CERAMDET) --> 
<!--         Air:      Air:     Si:    Boron Polyethyl.: Si:      Ceramic: -->
<!--  -->
 <Box name="SISTATION" dx="[XSi]/2" dy="[YSi]/2" dz="[ZSi]/2"/>
 <Box name="SIPLANE" dx="[XSiPlane]/2" dy="[YSiPlane]/2" dz="[ZSiPlane]/2"/>
 <Box name="SIDETL" dx="[XSiDet]/2" dy="[YSiDet]/2" dz="[ZSiDetL]/2"/>
 <Box name="BOUNDDET" dx="[XSiDet]/2" dy="[YSiDet]/2" dz="[ZBoundDet]/2"/>
 <Box name="SIDETR" dx="[XSiDet]/2" dy="[YSiDet]/2" dz="[ZSiDetR]/2"/>
 <Box name="CERAMDET" dx="[XSiDet]/2" dy="[YSiDet]/2" dz="[ZCeramDet]/2"/>
<!-- -->
 </SolidSection>
<!-- -->
<!-- -->
