setenv NRUN $1
setenv FILE conf${NRUN}.db

touch ${FILE}
rm -f ${FILE}
cmscond_export_iov -D CondFormatsDTObjects -P /afs/cern.ch/cms/DB/conddb -s oracle://cms_orcoff_prod/CMS_COND_21X_DT -d sqlite_file:${FILE} -t conf_ccb_V01 -b ${NRUN} -e ${NRUN}

sqlite3 ${FILE} << ENDOFCOMMANDS
select * from iov;
update iov set firstsince=1;
ENDOFCOMMANDS



