#!/bin/bash
url=`curl -s  http://cmssdt.cern.ch/SDT/html/showIB.html | egrep "CMSSW_[0-9]_[0-9]_CLANG_X_[0-9]+-[0-9]+-[0-9]+-[0-9]" - | grep showBuildLogs | head -1 | cut -d\" -f2 | sed 's/cgi-bin\/showBuildLogs.py/html\/rc/'`
[ -e /tmp/warning.txt ] || curl  $url"/release-build.log" | grep warning: > /tmp/warning.txt
[ -e /tmp/headers.txt ] || ./classes_extract 
[ -e /tmp/headers2.txt ] || ./classes_extract2 
for file in `cat /tmp/headers.txt`;do grep -F $file /tmp/warning.txt ; done | sort -u | sed 's/\/.*src\///' >/tmp/warning.headers.txt
for file in `cat /tmp/headers2.txt`;do grep -F $file /tmp/warning.txt ; done | sort -u | sed 's/\/.*src\///' >/tmp/warning.headers2.txt

